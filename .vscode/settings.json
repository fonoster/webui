{
  "debug.javascript.unmapMissingSources": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "files.exclude": {
    "**/.git": true,
    "**/dist": true,
    "**/node_modules": true,
    "**/tsconfig.tsbuildinfo": true,
    "**/yarn-error.log": true
  },
  "typescript.tsdk": "node_modules/typescript/lib"
}
